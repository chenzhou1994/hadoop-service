<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeninfo.hadoopservice.dao.DeptMapper">

  <!--新增-->
  <insert id="insert" parameterType="dept">
    insert into dept (id, deptName)
    values (#{id,jdbcType=INTEGER}, #{deptname,jdbcType=VARCHAR})
  </insert>

  <!--删除-->
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from dept
    where id = #{id,jdbcType=INTEGER}
  </delete>

  <!--修改-->
  <update id="updateByPrimaryKey" parameterType="dept">
    update dept
    set deptName = #{deptname,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>

  <!--根据主键查找-->
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultType="dept">
    select id, deptName
    from dept
    where id = #{id,jdbcType=INTEGER}
  </select>

  <!--获取所有-->
  <select id="selectAll" resultType="dept">
    select id, deptName
    from dept
  </select>
</mapper>